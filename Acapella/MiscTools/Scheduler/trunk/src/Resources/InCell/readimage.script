splitstring(imagefile,":")
rename(split=filenames)
set(basePath=filenames[0])
pop(filenames,pos=0)
set(basePath=regexreplace(basePath,"\\\\","/").text)
if(at("/",basePath,-1)!=basePath.length)
	set(basePath=basePath&"/")
end()
set(filenames=basePath&filenames)
Readers::InCell::filenames_to_images(filenames)