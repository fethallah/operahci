if(moduleName!="")
	set(moduleName=regexReplace(moduleName,"^(.*?)\\(.*","$1").text)
	set(moduleName=regexReplace(moduleName,".*?\\b(\\w*)$","$1").text)
	modules()
	functions()
	if(searchElem(moduleName,modules).pos>=0||searchElem(moduleName,functions).pos>=0)
		HtmlViewSession(0.5,newWindow=false,PollMode="push")
		HtmlView(label=moduleName & " - Module Description",php=ModuleDescriptionHTML(moduleName).moduledescription)
	end()
end()